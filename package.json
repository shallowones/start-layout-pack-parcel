{
  "name": "start-layout-pack-parcel",
  "version": "1.0.0",
  "description": "Репозиторий, который содержит сборку для начала верстки (parcel)",
  "main": "dist/pages/index.html",
  "scripts": {
    "start": "parcel src/pages/index.pug --out-dir build --no-cache",
    "build": "parcel build src/pages/*[!system] src/scripts/* src/styles/* --public-url ../ --no-cache",
    "postbuild": "node postbuild.js"
  },
  "author": "Pavel Belinovich",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "7.4.0",
    "@babel/plugin-proposal-class-properties": "7.4.0",
    "@babel/preset-env": "7.4.1",
    "deep-files": "2.0.1",
    "express": "4.17.1",
    "parcel-bundler": "1.12.4",
    "parcel-plugin-nuke-dist": "1.0.1",
    "prettier": "1.19.1",
    "pug": "2.0.4",
    "sass": "1.17.2",
    "stylelint": "12.0.1",
    "stylelint-declaration-use-variable": "1.7.2",
    "tslint": "6.0.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.1.0",
    "typescript": "3.7.5"
  },
  "dependencies": {
    "normalize.css": "8.0.1"
  }
}
